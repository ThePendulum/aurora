{
  "name": "aurora",
  "version": "1.39.2",
  "description": "WS28xx LED remote",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/ThePendulum/aurora.git"
  },
  "scripts": {
    "start": "node dist/app.js",
    "build": "webpack -p & babel src -d dist",
    "webpack": "webpack",
    "babel": "babel src -d dist",
    "watch-webpack": "webpack --progress --colors --watch",
    "watch-babel": "babel src -w -d dist",
    "doctoc": "doctoc . --notitle",
    "database-setup": "knex migrate:latest",
    "database-drop": "knex migrate:rollback",
    "database-fill": "knex seed:run",
    "users-add": "node dist/cli/createUser.js",
    "users-list": "node dist/cli/listUsers.js",
    "postinstall": "knex migrate:latest"
  },
  "keywords": [
    "ws28xx",
    "ws2801",
    "ws2812b",
    "ws2811",
    "led",
    "remote"
  ],
  "author": "Niels Simenon",
